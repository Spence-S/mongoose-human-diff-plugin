{
  "name": "mongoose-human-diff-plugin",
  "description": "See how your documents change in human readable form",
  "version": "0.0.1",
  "author": "Spencer Snyder <sasnyde2@gmail.com> (http://spencersnyder.io/)",
  "bugs": {
    "url": "https://github.com/Spence-S/mongoose-human-diff-plugin/issues",
    "email": "sasnyde2@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Spencer Snyder <sasnyde2@gmail.com> (http://spencersnyder.io/)"
  ],
  "dependencies": {
    "captain-hook": "^0.0.3",
    "human-object-diff": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "codecov": "latest",
    "cross-env": "7.0.0",
    "eslint": "latest",
    "eslint-config-xo-lass": "latest",
    "fixpack": "3.0.5",
    "husky": "latest",
    "lint-staged": "latest",
    "nyc": "latest",
    "remark-cli": "latest",
    "remark-preset-github": "latest",
    "xo": "latest"
  },
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/Spence-S/mongoose-human-diff-plugin",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "deep-diff",
    "deep-diff",
    "human-object-diff",
    "lass",
    "mongoose",
    "mongoose-diff",
    "mongoose-diff-plugin",
    "mongoose-plugin",
    "xo"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ],
    "*.md": [
      "remark . -qfo",
      "git add"
    ],
    "package.json": [
      "fixpack",
      "git add"
    ]
  },
  "main": "index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Spence-S/mongoose-human-diff-plugin"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo && remark . -qfo",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "yarn run lint && yarn run ava",
    "test-coverage": "yarn run lint && yarn run nyc"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  }
}
